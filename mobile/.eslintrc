{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["angular", "react"],
  "ecmaFeatures": {
	"modules":true,
	"arrowFunctions":true,
	"classes":true
  },
  "env": {
	"browser": true,
	"es6": true
  },
  "rules": {
	"jsx-indent": 0,
	"no-set-state": 0,
	"strict": ["error", "global"],
	"quotes": [
	  2,
	  "single"
	],
	"linebreak-style": [
	  2,
	  "unix"
	],
	"semi": [
	  2,
	  "always"
	]
  },
  "globals": {
	"google": true,
	"require": true,
	"_": true,
	"React": true,
	"ReactDOM": true,
	"angular": true,
	"module": true,
	"moment": true,
	"CountUp": true,
	"jQuery": true,
	"inject": true,
	"cordova": true,
	"ionic": true,
	"angularDragula": true
  }
}